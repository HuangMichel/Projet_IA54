/** 
 * 
 */
package utbm.ia54.ant2dgrid.objects
/** 
 * @author Michel
 * 
 */
class Cell {
	private var vector : Vector2D
	
	new(){
		this.vector = new Vector2D
	}
	
	new(v : Vector2D){
		this.vector = new Vector2D(v)
	}
	
	new(x : double, y : double){
		this.vector = new Vector2D(x, y)
	}
	
	def getPosition() : Vector2D {
		return vector
	}
	
	def setPosition(v : Vector2D){
		this.vector = v
	}
	
	def setPosition(x : double, y : double){
		this.vector.setX(x)
		this.vector.setY(y)
	}
}
