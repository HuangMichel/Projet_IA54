/** 
 * 
 */
package utbm.ia54.ant2dgrid.objects

import utbm.ia54.ant2dgrid.Enum.AntState

/** 
 * @author Michel
 * 
 */
class Pheromone {
	var type : AntState
	var intensity : int
	val coefEvaporation : float
	
	new(){
		this.type = AntState.SEARCH_FOOD
		this.intensity = 1
		this.coefEvaporation = 0.5f
	}
	
	new(type : AntState, intensity : int, coefEvaporation : float){
		this.type = type
		this.intensity = intensity
		this.coefEvaporation = coefEvaporation
	}
	
	
}
