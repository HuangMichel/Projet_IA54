/** 
 * 
 */
package utbm.ia54.ant2dgrid.objects
/** 
 * @author Michel
 * 
 */
class Vector2D {
	
	var x: double
	var y : double
	
	new(){
		this.x = 0
		this.y = 0
	}
	
	new(x : double, y : double){
		this.x = x;
		this.y = y
	}
	
	new(v : Vector2D){
		this.x = v.getX()
		this.y = v.getY()
	}
	
	def getX() : double {
		return x
	}
	
	def getY() : double {
		return y
	}
	
	def setX(x : double){
		this.x = x
	}
	
	def setY(y : double){
		this.y = y
	}
}
