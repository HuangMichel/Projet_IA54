 */
capacity MoveCapacity {
	def move(move : AntState)
	def moveUp(up : AntState)
	def moveDown(down : AntState)
	def moveLeft(left : AntState)
	def moveRight(right : AntState)
}

capacity PheromoneCapacity{
	def followPheromone()
}
